{
  "dependencies": {
    "babel-core": "^5.8.25",
    "bootstrap": "^3.3.5",
    "jquery": "^2.1.4",
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "scripts": {
    "copy:bootstrap-js": "cp -p node_modules/bootstrap/dist/js/bootstrap.min.js static/js/",
    "copy:bootstrap-css": "cp -p node_modules/bootstrap/dist/css/bootstrap.min.css static/css/",
    "copy:bootstrap": "npm run copy:bootstrap-js && npm run copy:bootstrap-css",
    "copy:jquery": "cp -p node_modules/jquery/dist/jquery.min.js static/js/",
    "copy:react": "cp -p node_modules/react/dist/react.js static/js/",
    "copy:react-dom": "cp -p node_modules/react-dom/dist/react-dom.js static/js/",
    "copy:babel": "cp -p node_modules/babel-core/browser.js static/js/",
    "copy": "npm run copy:bootstrap && npm run copy:jquery && npm run copy:react && npm run copy:react-dom && npm run copy:babel"
  }
}
